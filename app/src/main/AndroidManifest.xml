<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.together">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />
        <activity android:name=".TMapsActivity"></activity>
        <activity android:name=".TaxiMainActivity"></activity>
        <activity android:name=".MapsActivity"></activity>
        <activity android:name=".WeatherActivity"></activity>
        <activity android:name=".TaxiRoomActivity" />
        <activity android:name=".Set_user" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".LoginActivity" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".KtxMainActivity"/>
        <activity android:name=".KtxNewPostActivity"/>
        <activity android:name=".KtxChangePostActivity"/>
        <activity android:name=".TaxiNewPostActivity"/>
        <activity android:name=".TaxiChangePostActivity"/>
        <activity android:name=".KtxPostDetailActivity"/>
        <activity android:name=".TaxiPostDetailActivity"/>
        <activity android:name=".BusActivity"/>
        <activity android:name=".ChangePassword" />
        <activity android:name=".Auth"/>
        <activity android:name=".FindID" />
        <activity android:name=".FindIDcheck" />

    </application>

</manifest>